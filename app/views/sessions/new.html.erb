<div class="row">
	<div class="medium-12 columns">
		<h3>Log In </h3>
	</div>
</div>

<div class="row">
	<div class="medium-12 columns">
		<div id='session'>
			<%= form_for(:session, url: { action: 'create' }) do |f| %>
			  <div class="field"><%= f.text_field(:username, :size => 40, :placeholder => "Username") %></div>
			  <div class="field"><%= f.password_field :password, :size => 40, :placeholder => "Password" %></div>
			  <div class="actions"><%= f.submit 'Log In', class: "button medium" %></div>
			<% end %>

			<h5>Don't have an account?</h5>
			<%= link_to 'Sign up for an account', new_user_path %>
		</div>
	</div>
</div>





